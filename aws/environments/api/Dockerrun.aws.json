{
  "AWSEBDockerrunVersion": 2,
  "containerDefinitions": [
    {
      "name": "api",
      "image": "306439459454.dkr.ecr.us-west-2.amazonaws.com/backend_base",
      "command": [
        "./api.prod.sh"
      ],
      "essential": true,
      "memory": 1000,
      "portMappings": [
        {
          "hostPort": 8000,
          "containerPort": 8000
        }
      ]
    },
    {
      "name": "celery_flower",
      "image": "306439459454.dkr.ecr.us-west-2.amazonaws.com/backend_base",
      "command": [
        "./celery_flower.prod.sh"
      ],
      "essential": true,
      "memory": 100,
      "portMappings": [
        {
          "hostPort": 5555,
          "containerPort": 5555
        }
      ]
    },
    {
      "name": "celery_worker",
      "image": "306439459454.dkr.ecr.us-west-2.amazonaws.com/backend_base",
      "command": [
        "./celery_worker.sh"
      ],
      "essential": true,
      "memory": 300
    },
    {
      "name": "nginx",
      "image": "306439459454.dkr.ecr.us-west-2.amazonaws.com/nginx",
      "essential": true,
      "memory": 400,
      "portMappings": [
        {
          "hostPort": 80,
          "containerPort": 80
        }
      ],
      "links": [
        "api",
        "celery_flower"
      ]
    }
  ]
}
